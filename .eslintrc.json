{
    "env": {
        "browser": true,
        "es2021": true,
        "node":true,
        "jest/globals": true
        },
    "extends": ["eslint:recommended", "plugin:cypress/recommended"],
    "overrides": [
        {
            "files": ["**/*.cy.js"],
            "env":{ "cypress/globals":true},
            "plugins":["cypress"],
            "rules": {
              "cypress/no-unnecessary-waiting": "off",
              "no-unused-vars": "off"
              //"cypress/no-async-tests": "warn"
            }
          }
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn"
    },
        "plugins": ["jest"]
}
