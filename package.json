    "jest": "^29.2.2",
    "lint-staged": "^13.0.3",
    "live-server": "^1.1.0",
    "prettier": "2.7.1",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "bootstrap": "^5.2.2",
    "bootstrap-dark-5": "^1.1.3",
    "vite": "^3.2.3"
